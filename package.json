{
  "name": "vite-plugin-pyodide",
  "type": "module",
  "version": "0.0.12",
  "description": "Vite plugin that copy Python scripts into Pyodide virtual FS.",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./pyodide-files": {
      "types": "./dist/pyodide-files.d.ts"
    },
    "./*" : "./*"
  },
  "scripts": {
    "build": "tsc -p .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "plugin",
    "pyodide"
  ],
  "author": "Jiwon Choi",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "pyodide": "^0.26.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiwnchoi/vite-plugin-pyodide.git"
  },
  "bugs": {
    "url": "https://github.com/jiwnchoi/vite-plugin-pyodide/issues"
  },
  "homepage": "https://github.com/jiwnchoi/vite-plugin-pyodide#readme"
}